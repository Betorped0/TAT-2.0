}

<div>
    <div class="row">
        <div class="card-panel">
            <div class="row">
                <div class="row">
                    <div class="col s6 m6 l6">
                        <div class="col l12 m12 s12">
                            <div class="input-field col l10 m10" style="margin-bottom:0;">
                                @Html.EditorFor(model => model.KURST, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
